@*@model IEnumerable<KooliProjekt.Data.Storage>*@
@model PagedResult<StorageListModel>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

@*<p>
        <a asp-action="Create">Create New</a>
    </p>*@
<div>
    <table class="table">
        <thead>
            <tr>
                <th>
                    Code
                </th>
                <th>
                    Song
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.Results)
            {
            <tr>
                <td>
                    @item.Kood
                </td>
                <td>
                    @item.Song.Title
                </td>
                <td>
                    @*<a asp-action="Edit" asp-route-id="@item.StorageID">Edit</a> |*@
                    <a asp-action="Details" asp-route-id="@item.StorageID">Details</a> @*|
        <a asp-action="Delete" asp-route-id="@item.StorageID">Delete</a>*@
                </td>
            </tr>
            }
        </tbody>
    </table>
    <vc:pager result="Model" view-name="Default" />
</div>

